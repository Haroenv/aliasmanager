#!/usr/bin/env ruby


require_relative '../lib/aliasmanager'

command = ARGV[0]

manager = AliasManager.new(20)

case command
when 'unaliased'
  manager.most_used_commands_without_aliases
when 'aliased'
  manager.most_used_aliases
when 'total'
  manager.total
when 'sequences'
  manager.most_used_consecutive_commands
else
  manager.most_used_commands
end

